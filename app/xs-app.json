{
  "welcomeFile": "index.html",
  "authenticationMethod": "route",
  "logout": {
    "logoutEndpoint": "/do/logout"
  },
  "routes": [
    {
      "source": "(wrk/)(.*)",
      "destination": "job-sched_wrk_be",
      "csrfProtection": true,
      "authenticationType": "none"
    },
    {
      "source": "(util/)(.*)",
      "destination": "job-sched_srv_be",
      "csrfProtection": false,
      "authenticationType": "none"
    },
     {
      "source": "(sched/)(.*)",
      "destination": "job-sched_srv_be",
      "csrfProtection": true,
      "authenticationType": "xsuaa"
    },
    {
      "localDir": "resources",
      "source": "^/(.*)",
      "authenticationType": "none"
    }
  ]
}


